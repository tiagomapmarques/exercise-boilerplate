{
  "name": "exercise-boilerplate",
  "version": "0.0.1",
  "license": "MIT",
  "main": "src/main.tsx",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "postinstall": "playwright install --with-deps",
    "start": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:ci": "vitest --run",
    "format": "prettier . --write",
    "lint": "oxlint . --deny-warnings --fix",
    "check": "pnpm format && pnpm lint && pnpm build && pnpm test:ci"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.2"
  },
  "devDependencies": {
    "@testing-library/react": "^16.2.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-istanbul": "3.0.9",
    "oxlint": "^0.16.1",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9",
    "vitest-browser-react": "^0.1.1"
  }
}
